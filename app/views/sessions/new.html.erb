
    <%= form_for(:session, url: login_path) do |f| %>
      <%= f.select :name, options_from_collection_for_select(User.all, :id, :name), prompt: "Пользователь" %><br>
 		
      <%= f.password_field :password, :placeholder => 'Пароль' %>
      <br>
      <%= f.submit "Log in", :class => "form-btn" %>
      <br>
    <% end %>
 